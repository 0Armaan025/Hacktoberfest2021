# Write your code here
numlist=[]
esum=0
osum=0
number=int(input())
numlist=list(map(int,input().split())) 

for j in range(number) :
    if(numlist[j]%2==0 and j%2==0) :
        esum = esum + numlist[j]
    elif(numlist[j]%2!=0 and j%2!=0) :
        osum = osum + numlist[j]
        
print(esum , osum)
